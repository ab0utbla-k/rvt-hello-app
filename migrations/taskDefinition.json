{
  "containerDefinitions": [
    {
      "name": "migrate",
      "image": "794038210131.dkr.ecr.us-east-1.amazonaws.com/migrate-ecr:latest",
      "command": [
        "sh",
        "-c",
        "migrate -path /migrations -database \"$DB_DSN\" up"
      ],
      "secrets": [
        {
          "name": "DB_DSN",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:794038210131:secret:rds/postgres/dsn-I2NWsU"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/hello-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "migrate",
  "executionRoleArn": "arn:aws:iam::794038210131:role/hello-app-task-execution-role",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}